{
  "name": "karafriends",
  "version": "0.1.0",
  "private": true,
  "main": "build/prod/main/main.js",
  "scripts": {
    "build-native-dev": "cargo-cp-artifact --npm cdylib native/index.node -- cross-env-shell \"cargo build --message-format json-render-diagnostics --manifest-path native/Cargo.toml ${CARGO_ARGS:-}\"",
    "build-native-prod": "cargo-cp-artifact --npm cdylib native/index.node -- cross-env-shell \"cargo build --message-format json-render-diagnostics --manifest-path native/Cargo.toml --release ${CARGO_ARGS:-}\"",
    "build-relay-dev": "relay-compiler",
    "build-relay-prod": "relay-compiler",
    "build-electron-dev": "webpack --mode development",
    "build-electron-prod": "webpack --mode production",
    "build-frontend-dev": "parcel build --dist-dir build/dev --public-url . --no-optimize --no-scope-hoist",
    "build-frontend-prod": "parcel build --dist-dir build/prod --public-url . --no-source-maps",
    "build-dev": "concurrently \"yarn get-external-resources && yarn build-native-dev\" \"yarn build-relay-dev\" && concurrently \"yarn build-electron-dev\" \"yarn build-frontend-dev\"",
    "build-prod": "concurrently \"yarn get-external-resources && yarn build-native-prod\" \"yarn build-relay-prod\" && concurrently \"yarn build-electron-prod\" \"yarn build-frontend-prod\"",
    "run-dev": "cross-env NODE_ENV=development yarn _run-dev",
    "_run-dev": "yarn build-dev && concurrently \"parcel serve --dist-dir build/dev --port 3000\" \"wait-on http://127.0.0.1:3000/renderer/ && node ./electron.js build/dev/main/main.js\"",
    "package-prod": "node packager.js",
    "get-external-resources": "node scripts/getExternalResources.mjs",
    "test:native": "cargo clippy --manifest-path native/Cargo.toml && cargo +nightly careful test --manifest-path native/Cargo.toml",
    "test:wdio": "cross-env KARAFRIENDS_DEV_PORT=\"$(ephemeral-port-reserve)\" KARAFRIENDS_REMOCON_PORT=\"$(ephemeral-port-reserve)\" yarn _test:wdio",
    "_test:wdio": "concurrently --kill-others --success \"!command-0\" \"unbuffer -p parcel serve --dist-dir build/dev --port $KARAFRIENDS_DEV_PORT\" \"wait-on http://127.0.0.1:$KARAFRIENDS_DEV_PORT/renderer/ && pnpify wdio run renderer.wdio.conf.js && pnpify wdio run remocon.wdio.conf.js\"",
    "postinstall": "husky"
  },
  "dependencies": {
    "@graphql-tools/schema": "10.0.10",
    "@sentry/browser": "8.41.0",
    "@sentry/node": "8.41.0",
    "@types/react-fade-in": "2.0.2",
    "@types/react-swipeable": "5.2.0",
    "@types/user-agents": "1.0.4",
