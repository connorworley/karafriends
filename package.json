{
  "name": "karafriends",
  "version": "0.1.0",
  "private": true,
  "main": "build/main/main.js",
  "scripts": {
    "native-debug": "cross-env-shell \"cargo build --manifest-path native/Cargo.toml $CARGO_ARGS\"",
    "native-release": "cross-env-shell \"cargo build --manifest-path native/Cargo.toml --release --features asio $CARGO_ARGS\"",
    "compile": "relay-compiler --schema src/common/schema.graphql --src src --language typescript && webpack",
    "build": "yarn get-external-resources && yarn native-release && yarn compile && electron-builder -c.extraMetadata.main=./build/main/main.js --publish never --dir",
    "start": "cross-env NODE_ENV=development yarn _start",
    "_start": "yarn get-external-resources && yarn native-debug && yarn compile && concurrently \"webpack serve\" \"wait-on http://localhost:3000 && electron .\"",
    "get-external-resources": "node scripts/getExternalResources.mjs",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/react-fade-in": "^2.0.2",
    "@types/react-swipeable": "^5.2.0",
    "@types/user-agents": "^1.0.1",
    "@types/wicg-mediasession": "^1.1.3",
    "@types/youtube-player": "^5.5.3",
    "apollo-datasource-rest": "^0.13.0",
    "apollo-server-express": "^2.19.1",
    "classnames": "^2.3.1",
    "color-convert": "^2.0.1",
    "compression": "^1.7.4",
    "cubic-spline": "^3.0.3",
    "dataloader": "^2.0.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "format-duration": "^1.3.1",
    "gl-vec2": "^1.3.0",
    "graphql": "^15.4.0",
    "graphql-subscriptions": "^1.2.1",
    "hls.js": "^0.14.16",
    "keytar": "^7.7.0",
    "materialize-css": "^1.0.0-rc.2",
    "multicast-dns": "^7.2.2",
    "mv": "^2.1.1",
    "niconico": "^1.2.0",
    "node-fetch": "^2.6.1",
    "polyline-normals": "^2.0.2",
    "postcss": "8.4.14",
    "promise-retry": "^2.0.1",
    "qrcode": "^1.4.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fade-in": "^2.0.1",
    "react-icons": "^4.2.0",
    "react-mobile-picker-scroll": "^0.2.14",
    "react-relay": "^11.0.0",
    "react-router-dom": "^5.2.0",
    "react-swipeable": "^6.2.0",
    "regenerator-runtime": "^0.13.9",
    "relay-runtime": "^10.1.0",
    "subscriptions-transport-ws": "^0.9.18",
    "ts-invariant": "^0.9.1",
    "user-agents": "^1.0.658",
    "wanakana": "^4.0.2",
    "yaml": "^2.0.1",
    "youtube-player": "^5.5.2"
  },
  "devDependencies": {
    "7zip-bin": "^5.1.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/color-convert": "^2.0.0",
    "@types/express": "^4.17.9",
    "@types/format-duration": "^1.0.1",
    "@types/gl-vec2": "^1.3.1",
    "@types/hls.js": "^0.13.3",
    "@types/keytar": "^4.4.2",
    "@types/materialize-css": "^1.0.8",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/promise-retry": "^1.1.3",
    "@types/qrcode": "^1.3.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-relay": "^11.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.15.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^10.1.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^13.0.1",
    "electron-builder": "^22.9.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^5.0.4",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.2",
    "node-loader": "^2.0.0",
    "prettier": "2.2.1",
    "raw-loader": "^4.0.2",
    "relay-compiler": "^10.1.0",
    "relay-compiler-language-typescript": "^14.0.0",
    "relay-config": "^10.1.0",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2",
    "wait-on": "^5.2.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "resolutions": {
    "graphql": "^15.4.0"
  },
  "build": {
    "appId": "com.karafriends.kara",
    "extraResources": [
      "./extraResources/**"
    ],
    "files": [
      "build/**/**",
      "icon.png"
    ],
    "win": {
      "target": "portable",
      "icon": "icon.png"
    },
    "mac": {
      "icon": "icon.png"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.(ts|tsx)": "tslint --fix",
    "**/*.rs": "rustfmt"
  }
}
