{
  "name": "karafriends",
  "version": "0.1.0",
  "private": true,
  "main": "build/main/main.js",
  "scripts": {
    "build-native-dev": "cross-env-shell \"cargo build --manifest-path native/Cargo.toml ${CARGO_ARGS:-}\"",
    "build-native-prod": "cross-env-shell \"cargo build --manifest-path native/Cargo.toml --release ${CARGO_ARGS:-}\"",
    "build-relay-dev": "relay-compiler --schema src/common/schema.graphql --src src --language typescript",
    "build-relay-prod": "yarn build-relay-dev",
    "build-electron-dev": "yarn build-relay-dev && webpack --mode development",
    "build-electron-prod": "yarn build-relay-prod && webpack --mode   production",
    "build-frontend-dev": "yarn build-relay-dev && parcel build --dist-dir build --public-url . --no-optimize --no-scope-hoist",
    "build-frontend-prod": "yarn build-relay-prod && parcel build --dist-dir build --public-url .",
    "build-dev": "concurrently \"yarn get-external-resources\" \"yarn build-native-dev\" \"yarn build-relay-dev\" && concurrently \"yarn build-electron-dev\" \"yarn build-frontend-dev\"",
    "build-prod": "concurrently \"yarn get-external-resources\" \"yarn build-native-prod\" \"yarn build-relay-prod\" && concurrently \"yarn build-electron-prod\" \"yarn build-frontend-prod\" && electron-builder build --config.extraMetadata.main=./build/main/main.js --publish never --dir",
    "run-dev": "cross-env NODE_ENV=development yarn _run-dev",
    "_run-dev": "yarn build-dev && concurrently \"parcel serve --dist-dir build --port 3000\" \"wait-on http://127.0.0.1:3000/renderer/ && electron .\"",
    "get-external-resources": "node scripts/getExternalResources.mjs",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@sentry/browser": "^7.51.0",
    "@sentry/node": "^7.51.0",
    "@types/compression": "^1.7.2",
    "@types/react-fade-in": "^2.0.2",
    "@types/react-swipeable": "^5.2.0",
    "@types/user-agents": "^1.0.2",
    "@types/wicg-mediasession": "^1.1.4",
    "@types/youtube-player": "^5.5.3",
    "apollo-datasource-rest": "^0.13.0",
    "apollo-server-express": "^2.19.1",
    "classnames": "^2.3.1",
    "color-convert": "^2.0.1",
    "compression": "^1.7.4",
    "cubic-spline": "^3.0.3",
    "dataloader": "^2.2.2",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "format-duration": "^1.3.1",
    "gl-vec2": "^1.3.0",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^1.2.1",
    "hls.js": "^0.14.16",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "materialize-css": "^1.0.0",
    "multicast-dns": "^7.2.5",
    "mv": "^2.1.1",
    "niconico": "^1.2.0",
    "node-fetch": "^2.6.1",
    "polyline-normals": "^2.0.2",
    "postcss": "8.4.23",
    "promise-retry": "^2.0.1",
    "qrcode": "^1.5.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fade-in": "^2.0.1",
    "react-icons": "^4.8.0",
    "react-mobile-picker-scroll": "^0.2.14",
    "react-relay": "^11.0.0",
    "react-router-dom": "^5.2.0",
    "react-swipeable": "^7.0.0",
    "regenerator-runtime": "^0.13.11",
    "relay-runtime": "^10.1.0",
    "subscriptions-transport-ws": "^0.9.18",
    "ts-invariant": "^0.9.1",
    "user-agents": "^1.0.1371",
    "wanakana": "^5.1.0",
    "yaml": "^2.2.2",
    "youtube-player": "^5.5.2"
  },
  "devDependencies": {
    "7zip-bin": "^5.2.0",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.18.6",
    "@parcel/transformer-glsl": "2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "@types/color-convert": "^2.0.0",
    "@types/express": "^4.17.9",
    "@types/format-duration": "^1.0.1",
    "@types/gl-vec2": "^1.3.2",
    "@types/hls.js": "^0.13.3",
    "@types/keytar": "^4.4.2",
    "@types/lodash": "^4.14.194",
    "@types/materialize-css": "^1.0.11",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/promise-retry": "^1.1.3",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-relay": "^11.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.18.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-relay": "^10.1.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^18.3.7",
    "electron-builder": "^23.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "node-loader": "^2.0.0",
    "parcel": "^2.8.3",
    "prettier": "2.8.8",
    "process": "^0.11.10",
    "raw-loader": "^4.0.2",
    "relay-compiler": "^10.1.0",
    "relay-compiler-language-typescript": "^14.0.0",
    "relay-config": "^12.0.1",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "graphql": "^15.4.0"
  },
  "build": {
    "appId": "com.karafriends.kara",
    "extraResources": [
      "./extraResources/**"
    ],
    "files": [
      "build/**/**",
      "icon.png"
    ],
    "win": {
      "target": "portable",
      "icon": "icon.png"
    },
    "mac": {
      "icon": "icon.png",
      "entitlements": "entitlements.mac.plist"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.(ts|tsx)": "tslint --fix",
    "**/*.rs": "rustfmt"
  },
  "packageManager": "yarn@3.5.1",
  "targets": {
    "renderer": {
      "source": "src/renderer/index.html"
    },
    "remocon": {
      "source": "src/remocon/index.html"
    }
  }
}
