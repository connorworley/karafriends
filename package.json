  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.(ts|tsx)": "tslint --fix",
    "**/*.rs": "rustfmt"
  },
  "packageManager": "yarn@3.5.1",
  "targets": {
    "renderer": {
      "source": "src/renderer/index.html"
    },
    "remocon": {
      "source": "src/remocon/index.html"
    }
  },
  "relay": {
    "src": "src",
    "schema": "src/common/schema.graphql",
    "language": "typescript"
  },
  "alias": {
    "process": false
  }
}
